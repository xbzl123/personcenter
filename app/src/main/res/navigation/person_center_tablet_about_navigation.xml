<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/about_navigation"
    app:startDestination="@id/about"
    tools:ignore="MissingDefaultResource">

    <fragment
        android:id="@+id/about"
        android:name="com.module.personcenter.ui.AboutFragment">

        <action
            android:id="@+id/action_about_to_terms_condition"
            app:destination="@id/terms_condition" />
    </fragment>

    <fragment
        android:id="@+id/terms_condition"
        android:name="com.module.personcenter.ui.TermsConditionsFragment"/>
</navigation>